{
  "name": "reactnativepoc",
  "version": "1.0.0",
  "private": false,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "cross-env REACT_ENV=development expo start",
    "start:android": "cross-env REACT_ENV=development expo start --android",
    "start:ios": "cross-env REACT_ENV=development expo start --ios",
    "start:web": "cross-env REACT_ENV=development expo start --web",
    "optimize": "expo-optimize",
    "build:web:dev": "expo build:web --no-pwa",
    "build:web:test": "expo build:web --no-pwa",
    "build:web:prod": "expo build:web --no-pwa",
    "build:dev": "cross-env REACT_ENV=development run-s optimize build:web:dev",
    "build:test": "cross-env REACT_ENV=test run-s optimize build:web:test",
    "build:prod": "cross-env REACT_ENV=production run-s optimize build:web:prod",
    "serve": "serve web-build",
    "eject": "expo eject",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "^1.17.6",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "expo": "~45.0.0",
    "expo-asset": "~8.5.0",
    "expo-constants": "~13.1.1",
    "expo-font": "~10.1.0",
    "expo-linking": "~3.1.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-system-ui": "~1.2.0",
    "expo-web-browser": "~10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-storage": "^1.0.1",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "expo-optimize": "^0.2.16",
    "jest": "^26.6.3",
    "jest-expo": "~45.0.0",
    "npm-run-all": "^4.1.5",
    "react-native-dotenv": "^3.3.1",
    "react-test-renderer": "17.0.2",
    "serve": "^13.0.2",
    "sharp-cli": "^1.15.0",
    "typescript": "~4.3.5"
  }
}
